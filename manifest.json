{
    "name": "SelectSum",
    "version": "0.1",
    "manifest_version": 2,
    "description": "An extension that makes it easy to sum values",
    "permissions": [
      "contextMenus"
     ],
     "browser_action": {
         "default_popup": "src/html/popup.html"
     },
    "background": { 
      "scripts": ["src/js/background.js"]
    }
}